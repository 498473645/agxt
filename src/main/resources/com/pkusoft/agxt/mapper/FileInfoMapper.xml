    <!-- user-defined -->
    <!-- 通过用户id和案卷状态获取对象信息归还-->
    <select id="jobFileInfoListDataSMCJ" resultMap="BaseResultMapParam" parameterType="com.pkusoft.agxt.model.FileInfo">
        select * from(select s_.*,rownum rn_ from(
        select distinct
        f.*,count(distinct f.id) over () tot_cnt
        from FILE_INFO f
        inner join
        File_Auth auth
        on f.id = auth.file_id
        <where>
            1 = 1
<!--            ( 1 = 1 (-->
<!--            <if test=" operId!= null">-->
<!--                auth.OPER_ID = #{operId,jdbcType=VARCHAR}-->
<!--            </if>-->
<!--            <if test="orgCData != null">-->
<!--                auth.ORG_C_DATA = #{orgCData,jdbcType=VARCHAR}-->
<!--            </if>-->
<!--            <if test="orgSData != null">-->
<!--                auth.ORG_S_DATA = #{orgSData,jdbcType=VARCHAR}-->
<!--            </if>-->
<!--            <if test="orgTData != null">-->
<!--                auth.ORG_T_DATA = #{orgTData,jdbcType=VARCHAR}-->
<!--            </if>-->
<!--            &lt;!&ndash; <if test="orgCode != null" >-->
<!--              or auth.ORG_CODE = #{orgCode,jdbcType=VARCHAR}-->
<!--            </if> &ndash;&gt;-->

<!--            ) or (f.HOSTER_ID = #{zbrId,jdbcType=VARCHAR}) )-->
            <!-- 查询不等于9开头的数据,9999-作废和9000-归档都是不能查出来的,以后9开头的估计也不能查出来,所以排除9开头的 -->
            and f.STATUS NOT LIKE '9%'

            <if test=" status!= null">
                and f.status = #{status,jdbcType=VARCHAR}
            </if>
            <if test=" ajbh!= null">
                and f.CODE like #{ajbh,jdbcType=VARCHAR}
            </if>
            <if test=" saryxm != null">
                and f.KEYWORDS like #{saryxm,jdbcType=VARCHAR}
            </if>
            <if test=" ajmc!= null">
                and f.NAME like #{ajmc,jdbcType=VARCHAR}
            </if>
            <if test=" spaceId!= null">
                and f.SPACE_ID = #{spaceId,jdbcType=VARCHAR}
            </if>
            <if test=" hosterId!= null">
                and f.HOSTER_ID = #{hosterId,jdbcType=VARCHAR}
            </if>
            <if test=" handleOrgCode!= null">
                and f.HANDLE_ORG_CODE =#{handleOrgCode,jdbcType=VARCHAR}
            </if>
            <if test=" type!= null">
                and f.type = #{type,jdbcType=VARCHAR}
            </if>
            <if test=" createTimeq!= null">
                and f.CREATE_TIME &gt;= #{createTimeq,jdbcType=VARCHAR}
            </if>
            <if test=" createTimej!= null">
                and f.CREATE_TIME &lt;=#{createTimej,jdbcType=VARCHAR}
            </if>
        </where>
        order by f.MOD_TIME DESC
        ) s_ where rownum &lt;=#{start,jdbcType=DECIMAL}+#{pageSize,jdbcType=DECIMAL}) where rn_ &gt;#{start,jdbcType=DECIMAL}
    </select>

